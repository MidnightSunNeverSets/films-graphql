type Query {
    films: [Film!]!
    film(projectName: String!): Film
    actor(username: String!): Actor
    actors: [Actor!]!
}

type Film {
    title: String!
    projectName: String!
    releaseYear: Int!
    scenes: [Scene!]!
    cast: [Actor!]!
}

type Actor {
    username: String!
    name: String!
}

type Scene {
    id: String!
    description: String
}


